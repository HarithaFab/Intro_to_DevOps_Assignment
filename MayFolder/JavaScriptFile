// Define a function to greet the user
function greetUser(name) {
    console.log("Hello, " + name + "!");
}

// Call the function with a name
greetUser("John");

// Define an object representing a person
var person = {
    firstName: "Jane",
    lastName: "Doe",
    age: 30,
    hobbies: ["reading", "hiking", "painting"],
    greet: function() {
        console.log("Hello, my name is " + this.firstName + " " + this.lastName + ".");
    }
};

// Access properties and call methods of the person object
console.log("First Name:", person.firstName);
console.log("Age:", person.age);
console.log("Hobbies:", person.hobbies.join(", "));
person.greet();

// Define a simple class representing a car
class Car {
    constructor(brand, model, year) {
        this.brand = brand;
        this.model = model;
        this.year = year;
    }

    getFullDescription() {
        return this.year + " " + this.brand + " " + this.model;
    }
}

// Create an instance of the Car class
var myCar = new Car("Toyota", "Camry", 2020);

// Access properties and call methods of the car object
console.log("My Car:", myCar.getFullDescription());
